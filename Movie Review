var movies = [
    {
        title: "Puff the Magic Dragon",
        review: "Best movie ever!!"
    },
    {
        title: "Harry Potter and the Dealthy Hallows",
        review: "Better than Puff the Magic Dragon",
    }
];

for (var i = 0; i < movies.length; i++) {
    fill(84, 140, 209);
    textAlign(CENTER, CENTER);
    textSize(20);
    text(movies[i].title, 200, i*75+28);
    textSize(16);
    text(movies[i].review, 200, i*75 + 51);
}
